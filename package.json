    "@types/express": "^4.17.2",
    "@types/joi": "^14.3.4",
    "@types/node": "^13.5.0",
    "@types/restify": "^8.4.2",
    "@types/restify-errors": "^4.3.3",
    "@types/uuid": "^3.4.7",
    "ts-node": "^8.6.2",
